<ApiRequest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
<ApiKey>{{ apiKey }}</ApiKey>
<Data>
    <Receiver>
        <Email>{{ email }}</Email>
    </Receiver>
    <Snippets>
        {% for k,v in params %}
        <Snippet>
            <Name>{{ k }}</Name>
            <Value><![CDATA[ {{ v }} ]]></Value>
        </Snippet>
        {% endfor %}
    </Snippets>
</Data>
</ApiRequest>